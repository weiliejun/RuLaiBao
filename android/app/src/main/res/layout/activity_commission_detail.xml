                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:layout_marginLeft="16dp"
                android:text=""
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="22dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/individual_income_tax"
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>

            <TextView
                android:id="@+id/tv_individual_income_tax"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_weight="4"
                android:text=""
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="22dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/value_added_tax"
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>

            <TextView
                android:id="@+id/tv_value_added_tax"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_weight="4"
                android:text=""
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="22dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/additional_tax"
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>

            <TextView
                android:id="@+id/tv_additional_tax"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_weight="4"
                android:text=""
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="22dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/get_commission"
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>

            <TextView
                android:id="@+id/tv_get_commission"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:text=""
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="22dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/record_date"
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>

            <TextView
                android:id="@+id/tv_record_date"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:text=""
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="22dp"
            android:paddingBottom="45dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/settlement_time"
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>

            <TextView
                android:id="@+id/tv_settlement_time"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:text=""
                android:textColor="@color/txt_black2"
                android:textSize="16sp"/>
        </LinearLayout>

    </LinearLayout>

</ScrollView>